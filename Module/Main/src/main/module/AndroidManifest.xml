<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enzo.main">

    <application
        android:name="debug.ModuleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/main_news_name"
        android:supportsRtl="true"
        android:theme="@style/main_AppTheme">

        <activity
            android:name="com.enzo.main.ui.activity.SASplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.enzo.main.ui.activity.SAAdvertActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.enzo.main.ui.activity.SAMainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.enzo.main.ui.activity.SAAddDeviceActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>